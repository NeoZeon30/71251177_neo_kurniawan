      "source": [
        "#Menghitung nilai faktorial\n",
        "# Deklarasi\n",
        "var int a,b,n\n",
        "\n",
        "#Deskripsi\n",
        "a <- 0\n",
        "b <- 1\n",
        "INPUT n\n",
        "\n",
        "WHILE a <= n DO\n",
        "    a <- a + 1\n",
        "    b <- b * a\n",
        "END WHILE\n",
        "\n",
        "OUTPUT b\n"
      ]
    }
